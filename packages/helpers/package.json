{
	"name": "@omnysecurity/cognite-helpers",
	"version": "0.1.0",
	"private": false,
	"description": "Enhanced TypeScript support for Cognite Data Fusion APIs",
	"keywords": [
		"cognite",
		"cdf",
		"typescript",
		"data-fusion",
		"helpers",
		"sdk"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/omnysecurity/cognite-ts.git",
		"directory": "packages/helpers"
	},
	"homepage": "https://github.com/omnysecurity/cognite-ts#readme",
	"bugs": {
		"url": "https://github.com/omnysecurity/cognite-ts/issues"
	},
	"author": "Omny Security",
	"license": "Apache-2.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
		"format": "prettier --write src/"
	},
	"files": [
		"dist/",
		"!dist/**/*.test.*"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./filter": {
			"types": "./dist/filter.d.ts",
			"default": "./dist/filter.js"
		},
		"./helpers": {
			"types": "./dist/helpers.d.ts",
			"default": "./dist/helpers.js"
		}
	},
	"dependencies": {
		"@cognite/sdk": "^10.0.0"
	},
	"devDependencies": {
		"eslint": "catalog:",
		"prettier": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	}
}
