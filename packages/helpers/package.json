{
	"name": "@omnysecurity/cognite-helpers",
	"version": "0.0.0",
	"private": true,
	"description": "Enhanced TypeScript support for Cognite Data Fusion APIs",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
		"format": "prettier --write src/"
	},
	"files": [
		"dist/",
		"!dist/**/*.test.*"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"dependencies": {
		"@cognite/sdk": "^10.0.0"
	},
	"devDependencies": {
		"eslint": "catalog:",
		"prettier": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	}
}
