{
	"name": "@omnysecurity/cognite-codegen-cli",
	"version": "0.1.2",
	"private": false,
	"description": "Command-line interface for Cognite Data Fusion TypeScript code generation",
	"keywords": [
		"cognite",
		"cdf",
		"codegen",
		"cli",
		"typescript",
		"data-fusion",
		"data-models"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/omnysecurity/cognite-ts.git",
		"directory": "packages/codegen-cli"
	},
	"homepage": "https://github.com/omnysecurity/cognite-ts#readme",
	"bugs": {
		"url": "https://github.com/omnysecurity/cognite-ts/issues"
	},
	"author": "Omny Security",
	"license": "Apache-2.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"bin": {
		"cognite-codegen": "./dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
		"format": "prettier --write src/"
	},
	"files": [
		"dist/",
		"!dist/**/*.test.*",
		"src/"
	],
	"dependencies": {
		"@cognite/sdk": "^10.0.0",
		"@omnysecurity/cognite-codegen": "workspace:*",
		"meow": "^13.2.0",
		"typescript": "catalog:"
	},
	"devDependencies": {
		"@types/node": "^24.0.10",
		"eslint": "catalog:",
		"prettier": "catalog:",
		"vitest": "catalog:"
	}
